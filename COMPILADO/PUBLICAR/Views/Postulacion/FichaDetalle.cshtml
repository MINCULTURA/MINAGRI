                                    <select class="form-control" id="BTIPO_I">
                                        <option value="B">BÁSICO</option>
                                        <option value="I">INTERMEDIO</option>
                                        <option value="A">AVANZADO</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-10">
                                    <label class="control-label">Documento Sustento</label>
                                    <div id="divSelArchivo_IDI">
                                        <input type="file" name="UploadFile_IDI" id="txtUploadFile_IDI" />
                                    </div>
                                    <div id="divVerArchivo_IDI" style="display:none"></div>
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                        <button type="button" class="btn btn-primary" onclick="GuardarIdioma();">Grabar</button>
                    </div>
                </div>

            </div>
