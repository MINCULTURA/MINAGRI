                                    <div id="divTiempoServicio" style="font-weight:bold"></div>
                                </div>
                                <div class="col-lg-3">
                                    <div id="div_fechad_exp" class="input-append">
                                        <label class="control-label">Desde:</label>
                                        <input data-format="dd/MM/yyyy" type="text" class="form-control cls-fecha add-on" id="DFECHA_DESDE_EXP" maxlength="10" placeholder="DD/MM/AAAA">
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div id="div_fechah_exp" class="input-append">
                                        <label class="control-label">Hasta:</label>
                                        <input data-format="dd/MM/yyyy" type="text" class="form-control cls-fecha add-on" id="DFECHA_HASTA_EXP" maxlength="10" placeholder="DD/MM/AAAA">
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <input type="checkbox" id="BESPECIFICA" checked> Específica
                                    <br />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label class="control-label">Funciones principales:</label>
                                    <textarea class="form-control" rows="3" id="SFUNCIONES_EX" maxlength="3000"></textarea><br />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-3">
                                    <label class="control-label">N° presentaciones:</label>
                                    <input type="text" class="form-control solo-numero" id="NPRESENTACIONES" maxlength="3" style="text-align:right">
                                    <label style="font-size: 8px; font-family: arial; color: red;">(*)SOLO ELENCOS NACIONALES</label>
                                </div>
                                <div class="col-lg-9">
                                    <label class="control-label">Modalidad de contratación:</label>
                                    <input type="text" class="form-control letras-numeros" id="SMODALIDAD_EX" maxlength="1000"><br />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6">
                                    <label class="control-label">Motivo de retiro:</label>
                                    <textarea class="form-control letras-numeros" rows="3" id="SMOTIVORETIRO_EX" maxlength="1000"></textarea>
                                </div>
                                <div class="col-lg-6">
                                    <label class="control-label">Nombre y cargo del jefe directo:</label>
                                    <textarea class="form-control letras" rows="3" id="SNOMBRECARGOJEFE_EX" maxlength="1000"></textarea><br />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4">
                                    <label class="control-label">Remuneración o retribución:</label>
                                    <input type="number" class="form-control solo-numero" id="NREMUNERACION_EX" min="0">
                                </div>
                                <div class="col-lg-4">
                                    <label class="control-label">Teléfono oficina:</label>
                                    <input type="text" class="form-control solo-numero" id="STELEFONO_EX" maxlength="50">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label class="control-label">Documento Sustento</label>
                                    <div id="divSelArchivo_EXP">
                                        <input type="file" name="UploadFile_EXP" id="txtUploadFile_EXP" />
                                    </div>
                                    <div id="divVerArchivo_EXP" style="display:none"></div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="modal-footer">
                        @if (ViewBag.modo == "Editar")
                        {
                            <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>

                            <button type="button" class="btn btn-primary" onclick=" GuardarExp();">Grabar</button>
                        }
                    </div>
                </div>

            </div>
        </div>

        <script src="@Url.Content("~/js/Vistas/Postulacion/FichaDetalle.js?a=1")" type="text/javascript"></script>
