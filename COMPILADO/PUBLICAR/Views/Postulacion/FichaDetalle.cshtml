                        <div id="Experiencia">
                            <div class="row">
                                <div class="col-lg-6">
                                    <label class="control-label">Nombre de la entidad:</label>
                                    <input type="text" class="form-control letras-numeros" id="SENTIDAD_EX" maxlength="1000">
                                </div>
                                <div class="col-lg-6">
                                    <label class="control-label">Área:</label>
                                    <input type="text" class="form-control letras-numeros" id="SAREA_EX" maxlength="1000"><br />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-3">
                                    <label class="control-label">Sector:</label>
                                    <select class="form-control" id="BSECTOR">
                                        <option value="0">Seleccione</option>
                                        <option value="1">Público</option>
                                        <option value="2">Privado</option>
                                    </select>
                                </div>
                                <div class="col-lg-9">
                                    <label class="control-label">Cargo:</label>
                                    <input type="text" class="form-control letras-numeros" id="SCARGO_EX" maxlength="1000"><br />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-3">
