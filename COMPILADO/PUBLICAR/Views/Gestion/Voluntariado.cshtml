                        $('td:eq(7)', nRow).css("text-align", "center").css("width", "4%");
                        $('td:eq(8)', nRow).css("display", "none");
                        $('td:eq(9)', nRow).css("display", "none");
                        $('td:eq(10)', nRow).css("display", "none");
