                    data: inf,
                    "order": [
                                 [3, "desc"], [4, "asc"]
                    ],
                    "columnDefs": [
                        { "width": "0%", "targets": 0, "orderable": false },
                        { "width": "0%", "targets": 1, "orderable": false },
                        { "width": "0%", "targets": 2, "orderable": false },
                        { "width": "5%", "targets": 6, "orderable": false },
