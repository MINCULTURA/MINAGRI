@{
    ViewBag.Title = "Consulta General";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<input type="hidden" id="iduser" value="@ViewBag.NCODIGOUSUARIO">
<div class="row" style="padding-top:20px">
    <div class="col-lg-3">
        <label class="control-label">Convocatoria:</label>
        <select tabindex="-1" id="NIDCONVOCATORIA" class="form-control"></select>
    </div>
    <div class="col-lg-4">
        <label class="control-label">Proceso:</label>
        <select class="form-control" id="NIDPROCESO"></select>
    </div>
    <div class="col-lg-3">
        <label class="control-label">Estado:</label>
        <select class="form-control" id="BACTIVO_CONV">
            <option value="-1" selected>Todos</option>
            <option value="1" >Activo</option>
            <option value="0" >Inactivo</option>
        </select>
    </div>
    <div class="col-lg-2">
        <button type="button" class="btn btn-dark-grey" id="btn_Buscar" onclick="Buscar();">Buscar&nbsp;&nbsp;<i class="fa fa-search"></i> </button>
    </div>
</div>

<div class="row" style="padding-top:20px">
    <div class="col-lg-2">
        <div class="col-lg-12">  <input type="checkbox" id="chkEdad" name="chkEdad" onclick="habilitaEdad(this.checked);" /> Filtro por Edad</div>
        <div class="col-lg-6">
            Desde:&nbsp;&nbsp;<input type="text" class="form-control solo-numero" id="EDAD_DESDE" maxlength="2" style="text-align:right;" disabled>
        </div>
        <div class="col-lg-6">
            Hasta:&nbsp;&nbsp;<input type="text" class="form-control solo-numero" id="EDAD_HASTA" maxlength="2" style="text-align: right;" disabled>
        </div>
    </div>
    <div id="reset" >
    <div class="col-lg-2">
        Ingrese DNI
        <input type="text" class="form-control solo-numero" id="sdni">
    </div>
    <div class="col-lg-3">
        <button type="button" class="btn btn-dark-grey" id="btn_Resetear" onclick="ResetearClave();">Resetear Clave </button>
    </div>
        </div>
</div>
    <div class="row" style="padding-top:20px">
        <div class="col-lg-12">
            <div class="table-responsive" style="font-size:11px;">
                <table class="display compact" id="tbl_PostulantesE" cellspacing="0">
                    <thead>
                        <tr>
                            <th style="display:none; text-align:center;">NIDPROCESOUSU</th>
                            <th style="text-align:center; width:10%;">CONVOCATORIA</th>
                            <th style="text-align:center; width:3%;">EST.CONV.</th>
                            <th style="text-align:center; width:20%;">SERVICIO</th>
                            <th style="text-align:center; width:7%;">PROCESO</th>
                            <th style="text-align:center; width:5%;">RESULTADO</th>
                            <th style="text-align:center; width:10%;">POSTULANTE</th>
                            <th style="text-align:center; width:3%;">EST.FICHA</th>
                            <th style="text-align:center; width:3%;">F.NACIMIENTO</th>
                            <th style="text-align:center; width:2%;">EDAD</th>
                            <th style="text-align:center; width:3%;">SEXO</th>
                            <th style="text-align:center; width:20%;">EXP.LABORAL</th>
                            <th style="text-align:center; width:3%;">DNI</th>
                            <th style="text-align:center; width:3%;">RUC</th>
                            <th style="text-align:center; width:3%;">BREVETE</th>
                            <th style="text-align:center; width:10%;">DEPART. - PROV. - DIST.</th>
                            <th style="text-align:center; width:10%;">DIRECCIÓN</th>
                            <th style="text-align:center; width:3%;">TEL.FIJO</th>
                            <th style="text-align:center; width:3%;">CELULAR</th>
                            <th style="text-align:center; width:5%;">e-MAIL</th>
                            <th style="text-align:center; width:5%;">SIGLAS OFI.</th>
                            <th style="text-align:center; width:7%;">OFICINA</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
<script src="~/Scripts/dataTables.buttons.min.js"></script>
<script src="~/Scripts/buttons.print.min.js"></script>
<script src="~/Scripts/buttons.html5.min.js"></script>
<script src="~/Scripts/jszip.min.js"></script>
    <script src="@Url.Content("~/js/Vistas/Gestion/ConsultaGeneral.js")" type="text/javascript"></script>