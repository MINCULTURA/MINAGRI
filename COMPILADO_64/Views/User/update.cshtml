@{
    ViewBag.Title = "update";
    Layout = "~/Views/Shared/_Modal.cshtml";
}

<link rel="stylesheet" href="~/assets/plugins/bootstrap-fileupload/bootstrap-fileupload.min.css">
<script src="~/assets/plugins/bootstrap-fileupload/bootstrap-fileupload.min.js"></script>
<script src="@Url.Content("~/Scripts2/jquery-1.9.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/assets/plugins/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
<script type="text/javascript">
    function cargarFile() {
        var ruta = $("#txtruta").val();
        var file = $("#txname").val();
        var files = $("#file1_reg").get(0).files;
        var data = new FormData();
        var baseUrl = '@Url.Content("~")';
        for (i = 0; i < files.length; i++) {
            data.append("file" + i, files[i]);
        }
        $.ajax({
            type: "POST",
            url: baseUrl + 'User/CargaPostUPDATE?docfile=' + file + '&ruta=' + ruta,
            contentType: false,
            processData: false,
            data: data,
            success: function (result) {
                alert(result.msg);
            }
        });
    }
</script>

<h2>@ViewBag.ruta</h2>
<input id="txtruta" type="text">
<input type="file" id="file1_reg" multiple="multiple">
<input type="button" onclick="cargarFile()" value="cargar">
<input id="txname" type="text">